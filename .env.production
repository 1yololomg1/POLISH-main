# Production Environment Variables
# These will be overridden by Netlify environment variables

# API Configuration
VITE_API_BASE_URL=https://api.traceseis.com/polish/api

# Stripe Configuration (for payments)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_placeholder_replace_in_netlify_dashboard

# File Size Limits
VITE_MAX_FILE_SIZE=104857600

# Feature Toggles
VITE_FEATURE_EXPORT=true
VITE_FEATURE_CONVERSION=true
VITE_FEATURE_REPORTS=true
VITE_FEATURE_MULTI_WELL=true
VITE_FEATURE_ADVANCED_QC=true
VITE_FEATURE_GEOLOGICAL_ANALYSIS=true

# Payment System Toggle
VITE_PAYMENT_SYSTEM_ENABLED=true